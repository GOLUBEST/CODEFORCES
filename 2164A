#include <bits/stdc++.h>
using namespace std;
int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int T;
    if (!(cin >> T)) return 0;
    while (T--) {
        int n;
        cin >> n;
        vector<long long> a(n);
        for (int i = 0; i < n; ++i) cin >> a[i];
        long long x; cin >> x;
        long long mn = a[0], mx = a[0];
        for (int i = 1; i < n; ++i) {
            mn = min(mn, a[i]);
            mx = max(mx, a[i]);
        }
        if (x >= mn && x <= mx) cout << "YES\n"; else cout << "NO\n";
    }
    return 0;
}
