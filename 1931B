#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;

    while (t--) {
        int n ;
        long long sum = 0;
        cin >> n;
        
        int arr[n];
        for(int i = 0 ; i < n ; i++) {
            cin >> arr[i];
            sum += arr[i];
        }

        long long avg = sum / n;
        long long total = 0;
        int flag = 0;

        for(int i = 0 ; i < n ; i++) {
            total += arr[i];
            if(total < (long long)(i+1) * avg){
                flag = 1;
                break;
            }
        }
        cout << (flag ? "No" : "Yes") << endl;
    }

    return 0;
}
