#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    if (!(cin >> t)) return 0;
    while (t--) {
        unsigned long long n;
        cin >> n;
        unsigned long long ans = 2 * n - (unsigned long long)__builtin_popcountll(n);
        cout << ans << '\n';
    }
    return 0;
}
