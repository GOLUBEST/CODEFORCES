#include <bits/stdc++.h>
using namespace std;

int main() {
    int T;
    if (!(cin >> T)) return 0;
    while (T--) {
        int n;
        string s;
        cin >> n >> s;
        int z = 0;
        for (char c : s) if (c == '0') ++z;
        int m = 0;
        for (int i = 0; i < z; ++i) if (s[i] == '1') ++m;
        cout << m << endl;
    }
    return 0;
}
