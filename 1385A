#include<bits/stdc++.h>
#define ll long long int
using namespace std;

int main()
{
	int t=1;
	cin>>t;
	while(t--)
	{
    	ll x,y,z,a=0,b=0,c=0;
    	cin>>x>>y>>z;
    	if(x==y)
    	{
    		a=x;
    	}
    	if(x==z)
    	{
    		b=x;
    	}
    	if(y==z)
    	{
    		c=z;
    	}
    	if(a==0)
    		a=min(x,y);
    	if(b==0) 
    	    b=min(x,z);
    	if(c==0) 
    	    c=min(y,z);
    	if(max(a,b)!=x || max(b,c)!=z || max(a,c)!=y)
    		cout<<"NO"<<endl;
    	else
    	{
    		cout<<"YES"<<endl;
    		cout<<a<<" "<<b<<" "<<c<<endl;
    	}
	}
    return 0;
}
