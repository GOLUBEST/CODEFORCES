#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;
    if (!(cin >> t)) return 0;
    while (t--) {
        int n;
        cin >> n;
        vector<char> seen(n + 1, 0);
        int cnt = 0;
        for (int i = 0; i < n; ++i) {
            int x;
            cin >> x;
            if (!seen[x]) {
                seen[x] = 1;
                ++cnt;
            }
        }
        cout << cnt << "\n";
    }
    return 0;
}
