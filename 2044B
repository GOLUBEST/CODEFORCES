#include<bits/stdc++.h>
using namespace std;
 
int main() {
    int t;
    cin >> t;
    
    while(t--) {
        string s;
        cin >> s;
        string ans;
        int n = s.length();
        for(int i = n-1 ; i >= 0 ; i--){
            if(s[i] == 'q')
                ans.push_back('p');
            else if(s[i] == 'p')
                ans.push_back('q');
            else
                ans.push_back('w');
        }
        cout << ans << endl;
    }
}
