#include <iostream>
#include <string>
using namespace std;

int main() {
    int t;
    cin >> t;
    string pi = "314159265358979323846264338327";
    while (t--) {
        string n;
        cin >> n;
        int cnt = 0;
        for (int i = 0; i < n.size(); ++i) {
            if (i >= pi.size()) break;
            if (n[i] == pi[i]) cnt++;
            else break;
        }
        cout << cnt << endl;
    }
    return 0;
}
