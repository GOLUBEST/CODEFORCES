#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    if (!(cin >> t)) return 0;
    while (t--) {
        int n;
        string s;
        cin >> n >> s;

        for (char &c : s) c = tolower(c);

        string comp;
        for (char c : s) {
            if (comp.empty() || comp.back() != c) comp.push_back(c);
        }

        cout << (comp == "meow" ? "YES\n" : "NO\n");
    }
    return 0;
}
