
// IS IT A CAT


#include<bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;

    while (t--) {
        int n;
        cin >> n;

        string s;
        cin >> s;

        int cnt = 0, flag = 0, j = 0;
        int m = 0;

        for (int i = 0; i < n; i++) {
            if (cnt == 0 && (s[i] == 'm' || s[i] == 'M')) {
                m = 1;
                continue;
            }
            else if (cnt == 0 && m && (s[i] == 'e' || s[i] == 'E')) {
                cnt = 1;
                continue;
            }
            else if (cnt == 1 && (s[i] == 'e' || s[i] == 'E')) {
                continue;
            }
            else if (cnt == 1 && (s[i] == 'o' || s[i] == 'O')) {
                cnt = 2;
                continue;
            }
            else if (cnt == 2 && (s[i] == 'o' || s[i] == 'O')) {
                continue;
            }
            else if (cnt == 2 && (s[i] == 'w' || s[i] == 'W')) {
                j = 1;
                continue;
            }
            else {
                flag = 1;
                break;
            }
        }

        if (!flag && m && cnt == 2 && j == 1) {
            cout << "YES\n";
        } else {
            cout << "NO\n";
        }
    }
    return 0;
}
