#include <bits/stdc++.h>
using namespace std;
using int64 = long long;

inline long long count_good_upto(long long x) {
    if (x <= 0) return 0;
    static const int p[4] = {2, 3, 5, 7};
    long long res = x;
    for (int mask = 1; mask < 16; ++mask) {
        long long d = 1;
        int bits = 0;
        for (int i = 0; i < 4; ++i) {
            if (mask & (1 << i)) {
                d *= p[i];
                ++bits;
            }
        }
        if (bits & 1) res -= x / d;
        else          res += x / d;
    }
    return res;
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--) {
        long long l, r;
        cin >> l >> r;
        cout << (count_good_upto(r) - count_good_upto(l - 1)) << '\n';
    }
    return 0;
}
