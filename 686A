#include <bits/stdc++.h>
using namespace std;

int main() {
    int n; 
    long long x;
    cin >> n >> x;
    long long d, distress = 0;
    char c;
    while(n--) {
        cin >> c >> d;
        if(c == '+') 
            x += d;
        else {
            if(x >= d) 
                x -= d;
            else 
                distress++;
        }
    }
    cout << x << " " << distress;
}
