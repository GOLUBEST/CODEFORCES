#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    if (!(cin >> t)) return 0;
    while (t--) {
        string pos;
        cin >> pos;
        char col = pos[0];
        char row = pos[1];
        for (char c = 'a'; c <= 'h'; ++c) {
            if (c == col) continue;
            cout << c << row << endl;
        }
        for (char r = '1'; r <= '8'; ++r) {
            if (r == row) continue;
            cout << col << r << endl;
        }
    }
    return 0;
}
