#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    
    int t;
    if (!(cin >> t)) return 0;
    while (t--) {
        int n, k;
        cin >> n >> k;
        vector<string> a(n);
        for (int i = 0; i < n; ++i) cin >> a[i];
        
        int m = n / k;
        for (int bi = 0; bi < m; ++bi) {
            string row;
            for (int bj = 0; bj < m; ++bj) {
                row.push_back(a[bi * k][bj * k]);
            }
            cout << row << '\n';
        }
    }
    return 0;
}
