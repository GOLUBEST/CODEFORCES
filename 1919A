#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    
    int t;
    if (!(cin >> t)) return 0;
    while (t--) {
        long long a, b;
        cin >> a >> b;
        long long s = a + b;
        if (s % 2 == 1) cout << "Alice\n";
        else cout << "Bob\n";
    }
    return 0;
}
