#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;
    if (!(cin >> t)) return 0;
    while (t--) {
        long long n;
        cin >> n;
        long long ans = 0;
        long long p = 1;
        while (p <= n) {
            ans += min(9LL, n / p);
            p *= 10;
        }
        cout << ans << '\n';
    }
    return 0;
}
