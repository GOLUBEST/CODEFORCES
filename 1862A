#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;
    if (!(cin >> t)) return 0;
    while (t--) {
        int n, m;
        cin >> n >> m;
        vector<string> g(n);
        for (int i = 0; i < n; ++i) cin >> g[i];
        string want = "vika";
        int idx = 0;
        for (int col = 0; col < m && idx < 4; ++col) {
            for (int row = 0; row < n; ++row) {
                if (g[row][col] == want[idx]) {
                    ++idx;
                    break; // move to next column after matching char
                }
            }
        }
        cout << (idx == 4 ? "YES" : "NO") << '\n';
    }
    return 0;
}
