#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        vector<int> freq(n + 1, 0);
        for (int i = 0; i < n; ++i) {
            int a; cin >> a;
            freq[a]++;
        }
        int score = 0;
        for (int x = 1; x <= n; ++x) score += freq[x] / 2;
        cout << score << '\n';
    }
    return 0;
}
