#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int q;
    if (!(cin >> q)) return 0;
    while (q--) {
        int n;
        string s, t;
        cin >> n >> s >> t;

        int cnt[26] = {0};
        for (char c : s) cnt[c - 'a']++;
        for (char c : t) cnt[c - 'a']--;

        bool ok = true;
        for (int i = 0; i < 26; ++i) {
            if (cnt[i] != 0) { ok = false; break; }
        }
        cout << (ok ? "YES" : "NO") << "\n";
    }

    return 0;
}
