#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--) {
        int n, j, k;
        cin >> n >> j >> k;
        vector<int> a(n);
        for (int i = 0; i < n; i++) cin >> a[i];

        if (k > 1) {
            cout << "YES\n";
            continue;
        }
        int mx = *max_element(a.begin(), a.end());
        cout << (a[j - 1] == mx ? "YES\n" : "NO\n");
    }
    return 0;
}
