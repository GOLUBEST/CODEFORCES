#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    if (!(cin >> t)) return 0;
    while (t--) {
        int n, m;
        cin >> n >> m;
        int sum = 0, x = 0;
        for (int i = 0; i < n; ++i) {
            string s;
            cin >> s;
            if (sum + (int)s.size() <= m) {
                sum += (int)s.size();
                ++x;
            }
        }
        cout << x << "\n";
    }
    return 0;
}
