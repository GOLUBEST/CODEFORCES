#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--) {
        long long p, q;
        cin >> p >> q;

        long long K = min(p / 2, q / 3);

        if (K == 0) {
            cout << "Alice\n";
        } else if (K >= 2) {
            cout << "Bob\n";
        } else {
            cout << (( (p + q) % 2 == 0 ) ? "Bob\n" : "Alice\n");
        }
    }
    return 0;
}
